print("Mapping");
var map = function(l, f, length){
    for(var i = 0; i < length; i = i+1){
	l[i] = f(l[i]);
    };
    return;
};

var square = function(x){
    return x*x;
};

var l = [1,2,3];

map(l, square, 3);

foreach(i = 0<3){
    print(l[i]);
};